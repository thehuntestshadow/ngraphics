{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "node",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "checkJs": false,
    "allowJs": true,
    "strict": false,
    "strictNullChecks": false,
    "strictFunctionTypes": true,
    "noImplicitThis": false,
    "noEmit": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "*": ["./*"]
    }
  },
  "include": [
    "*.js"
  ],
  "exclude": [
    "node_modules",
    "service-worker.js",
    "image-worker.js"
  ]
}
